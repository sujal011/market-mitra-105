{
  "expo": {
    "name": "market-mitra-app",
    "slug": "market-mitra-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEF9ED"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marketmitra",
      "googleServicesFile": "./GoogleServices-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FEF9ED"
      },
      "package": "com.marketmitra",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with us.",
          "cameraPermission": "The app accesses your camera for photos to let you share them with us."
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameWorks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.159697299149-k7osethqbblcsofe7ul123u5c479euch"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "466ae6a4-23e7-4a7c-b2a9-819811af4423"
      }
    }
  }
}
